{
    "name": "restjs/php-rest-api",
    "description": "Create Rest API Application",
    "type": "package",
    "keywords": [
        "php",
        "composer",
        "rest-api",
        "file-upload",
        "web-services",
        "jwt-authentication",
        "my-sql"
    ],
    "homepage": "https://github.com/fullstackondemand/php-rest-api",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RestJS\\PhpRestApi\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "RestJS/php-rest-api",
                "version": "v2",
                "source": {
                    "url": "https://github.com/fullstackondemand/php-rest-api.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "authors": [
        {
            "name": "Pramod Singh",
            "email": "spramodgusain@gmail.com",
            "homepage": "https://github.com/fullstackondemand",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "7.1.*|7.2.*|7.3.*|7.4.*|8.0.*|8.1.*",
        "mongodb/mongodb": "^1.12",
        "alcaeus/mongo-php-adapter": "^1.2",
        "vlucas/phpdotenv": "^5.6"
    }
}