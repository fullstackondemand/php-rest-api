    "keywords": [
        "php",
        "composer",
        "rest-api",
        "file-upload",
        "web-services",
        "jwt-authentication",
        "my-sql",
        "slim",
        "doctrine-orm",
        "symfony-cache",
        "php-di-container",
        "middlewares-trailing-slash"
    ],
    "homepage": "https://github.com/fullstackondemand/php-rest-api",
